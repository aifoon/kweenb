{
  "name": "kweenb",
  "version": "1.0.7",
  "private": true,
  "description": "KweenB is an audio management application for the Zwerm3 project of Aifoon (BE).",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./src/main && vite build",
    "build:preload": "cd ./src/preload && vite build",
    "build:renderer": "cd ./src/renderer && vite build",
    "watch": "node scripts/watch.js",
    "prepare-publish": "node scripts/prepare_publish.js",
    "publish": "npm run prepare-publish && npm run compile && node scripts/publish.js",
    "publish:linux": "npm run prepare-publish  && npm run compile:linux && node scripts/publish.js",
    "publish-test": "node --max-old-space-size=4096 scripts/publish.js",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js",
    "compile:linux": "MODE=production npm run build && MODE=production USE_SYSTEM_FPM=true electron-builder build --config .electron-builder.config.js --linux",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "typecheck:main": "tsc --noEmit -p src/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p src/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p src/renderer/tsconfig.json"
  },
  "main": "src/main/dist/index.cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/aifoon/kweenb"
  },
  "keywords": [],
  "author": "aifoon <info@aifoon.be>",
  "license": "ISC",
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@electron/rebuild": "^3.2.10",
    "@octokit/rest": "^19.0.5",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^18.11.12",
    "@types/ping": "^0.4.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-context-menu": "^3.1.1",
    "electron-debug": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3",
    "winston": "^3.8.2"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/base": "^5.0.0-alpha.74",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@mui/styled-engine-sc": "^5.1.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/source-map-support": "^0.5.4",
    "@types/styled-components": "^5.1.17",
    "@vitejs/plugin-react": "^3.0.0",
    "@zwerm3/jack": "^1.2.19",
    "aedes": "^0.47.0",
    "chalk": "^4.1.2",
    "deep-equal": "^2.0.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.7",
    "eslint-import-resolver-typescript": "^2.5.0",
    "formik": "^2.2.9",
    "glamor": "^2.17.9",
    "mqtt": "^4.3.7",
    "node-fetch": "^3.3.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "ping": "^0.4.1",
    "react": "^17.0.2",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1",
    "react-scroll-to-bottom": "^4.2.0",
    "sequelize": "^6.16.1",
    "sqlite3": "^5.0.8",
    "styled-components": "^5.3.3",
    "vite": "^4.0.0",
    "vite-tsconfig-paths": "^3.3.17",
    "wildcard-match": "^5.1.2",
    "yup": "^0.32.11"
  },
  "browserslist": [
    "last 1 electron version"
  ]
}
